+import 'package:juan_tracker/app.dart';
+
+void main() {
+  testWidgets('Home shows greeting', (WidgetTester tester) async {
+    await tester.pumpWidget(const ProviderScope(child: JuanTrackerApp()));
+
+    expect(find.text('Hello'), findsOneWidget);
+  });
